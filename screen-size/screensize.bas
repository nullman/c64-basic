10 REM Screen Size
20 PRINT CHR$(147);
100 FOR X = 1 TO 40
110 IF X - INT(X / 10) * 10 = 0 THEN 120
111 PRINT " ";
112 GOTO 130
120 PRINT CHR$(48 + X / 10);
130 NEXT X
200 FOR T = 1 TO 4
210 FOR X = 1 TO 10
220 IF X = 10 THEN PRINT "0";
220 IF X <> 10 THEN PRINT CHR$(48 + X);
230 NEXT X
240 NEXT T
300 FOR Y = 3 TO 25
310 IF Y > 3 THEN PRINT
320 PRINT Y;
330 NEXT Y
400 INPUT Z$
