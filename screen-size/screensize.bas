10 REM Screen Size
20 PRINT CHR$(147);
30 FOR X = 1 TO 40
40 IF X - INT(X / 10) * 10 = 0 THEN 70
50 PRINT " ";
60 GOTO 80
70 PRINT CHR$(48 + X / 10);
80 NEXT X
90 FOR T = 1 TO 4
100 FOR X = 1 TO 10
120 IF X = 10 THEN PRINT "0";
120 IF X <> 10 THEN PRINT CHR$(48 + X);
130 NEXT X
140 NEXT T
150 FOR Y = 3 TO 25
160 IF Y > 3 THEN PRINT
170 PRINT Y;
180 NEXT Y
190 INPUT Z$
